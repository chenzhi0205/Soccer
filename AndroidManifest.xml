<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.whs.soccer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="14" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.whs.soccer.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.whs.soccer.ProfileActivity"
            android:label="@string/title_activity_profile" >
        </activity>
        <activity
            android:name="com.whs.soccer.TeamActivity"
            android:label="@string/title_activity_team" >
        </activity>
        <activity
            android:name="com.whs.soccer.MatchActivity"
            android:label="@string/title_activity_match" >
        </activity>
        <activity
            android:name="com.whs.soccer.ReserveActivity"
            android:label="@string/title_activity_reserve" >
        </activity>
        <activity
            android:name="com.whs.soccer.FinanceActivity"
            android:label="@string/title_activity_finance" >
        </activity>
    </application>

</manifest>
